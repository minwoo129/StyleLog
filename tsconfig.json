{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@pages/*": ["src/pages/*"],
      "@components/*": ["src/components/*"],
      "@utils/*": ["src/utils/*"],
      "@hooks/*": ["src/hooks/*"],
      "@config": ["src/config"],
      "@config/*": ["src/config/*"],
      "@contexts": ["src/contexts"],
      "@contexts/*": ["src/contexts/*"],
      "@constants/*": ["src/constants/*"],
      "@themes/*": ["src/themes/*"]
    }
  },
  "extends": "@react-native/typescript-config/tsconfig.json"
}
